	            </if>
	            <if test="checkstatus != null">
	               and check_status = #{checkstatus}
	            </if>
	            <if test="startdate != null">
	            	<![CDATA[
	              	 and regist_time >= #{startdate}
	              	]]>
	            </if>
	            <if test="enddate != null">
	               <![CDATA[
	              	 and regist_time <= #{enddate}
	               ]]>
	            </if>
	        </where>) a
	        <where>
      		 	<if test="primarkey != null">
		             <bind name="pattern" value="'%' + primarkey + '%'" />
		             <foreach collection="primarkeys" open=" " item="item" separator=" " close=" ">
			            or a.${item} like #{pattern}
			          </foreach>
          		</if>
