<!-- CONFIG_PUBLIC_CHAR -->
<mapper namespace="com.hr.mapper.ConfigPublicCharMapper">
    <!-- This code was generated by TableGo tools, mark 1 begin. -->
    <!-- 字段映射 -->
    <resultMap id="configPublicCharMap" type="ConfigPublicChar">
        <id column="PBC_ID" property="pbcId" jdbcType="VARCHAR" />
        <result column="ATTRIBUTE_KIND" property="attributeKind" jdbcType="VARCHAR" />
        <result column="ATTRIBUTE_NAME" property="attributeName" jdbcType="VARCHAR" />
    </resultMap>
    <!-- This code was generated by TableGo tools, mark 1 end. -->
    
    <!-- This code was generated by TableGo tools, mark 2 begin. -->
    <!-- 表所有字段 -->
    <sql id="allColumns">
        PBC_ID, ATTRIBUTE_KIND, ATTRIBUTE_NAME
    </sql>
    <!-- This code was generated by TableGo tools, mark 2 end. -->
    
    <!-- 查询所有数据 -->
    <select id="findConfigPublicCharAll" resultMap="configPublicCharMap">
        SELECT
        <include refid="allColumns" />
        FROM CONFIG_PUBLIC_CHAR
    </select>
    
    <!-- 根据主键查询数据 -->
    <select id="findConfigPublicCharByIds" resultMap="configPublicCharMap" parameterType="list">
        SELECT
        <include refid="allColumns" />
        FROM CONFIG_PUBLIC_CHAR WHERE PBC_ID IN
        <foreach collection="list" index="index" item="pbcId" open="(" separator="," close=")">
            #{pbcId}
        </foreach>
    </select>
    
    <!-- 插入数据 -->
    <insert id="insertConfigPublicChar" parameterType="map">
        INSERT INTO CONFIG_PUBLIC_CHAR (
            PBC_ID, ATTRIBUTE_KIND, ATTRIBUTE_NAME
        ) VALUES (
            #{pbcId},
            #{attributeKind},
            #{attributeName}
        )
    </insert>
    
    <!-- 修改数据 -->
    <update id="updateConfigPublicChar" parameterType="map">
        UPDATE CONFIG_PUBLIC_CHAR
        <set>
            <if test="attributeKind != null">
                ATTRIBUTE_KIND = #{attributeKind},
            </if>
            <if test="attributeName != null">
                ATTRIBUTE_NAME = #{attributeName}
            </if>
        </set>
        WHERE PBC_ID = #{pbcId}
    </update>
    
    <!-- 根据主键删除数据 -->
    <delete id="deleteConfigPublicCharById" parameterType="string">
        DELETE FROM CONFIG_PUBLIC_CHAR WHERE PBC_ID = #{pbcId}
    </delete>
    
